{
    "name": "@dintero/node-sdk",
    "version": "0.0.0-development",
    "description": "Node.js library for the Dintero API",
    "main": "./dist/dintero.js",
    "typings": "./dist/dintero.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "author": "Dintero <support@dintero.com> (https://dintero.com/)",
    "jest": {
        "preset": "ts-jest",
        "testMatch": [
            "**/test/**/*.test.ts"
        ]
    },
    "scripts": {
        "build": "yarn -s build-ts && yarn -s lint",
        "build-ts": "tsc -p ./tsconfig.build.json",
        "test": "jest",
        "lint": "biome check",
        "convert-spec": "ts-node -T scripts/convert-spec.ts",
        "semantic-release": "semantic-release",
        "prepublishOnly": "yarn run build-ts"
    },
    "devDependencies": {
        "@biomejs/biome": "2.0.6",
        "@types/jest": "30.0.0",
        "@types/js-yaml": "4.0.9",
        "@types/node": "24.0.10",
        "@types/swagger2openapi": "7.0.4",
        "jest": "30.0.3",
        "js-yaml": "4.1.0",
        "msw": "2.10.2",
        "openapi-typescript": "7.8.0",
        "semantic-release": "24.2.6",
        "swagger2openapi": "7.0.8",
        "ts-jest": "29.4.0",
        "ts-node": "10.9.2",
        "typescript": "5.8.3"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "openapi-fetch": "^0.14.0"
    }
}
