        "@types/swagger2openapi": "7.0.4",
        "jest": "29.7.0",
        "js-yaml": "4.1.0",
        "msw": "2.4.2",
        "openapi-typescript": "^7.4.0",
        "swagger2openapi": "7.0.8",
        "ts-jest": "29.2.5",
        "ts-node": "^10.9.2",
        "typescript": "^5.6.2",
        "undici": "6.19.6"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "openapi-fetch": "0.12.0"
    }
