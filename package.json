{
    "name": "@dintero/node-sdk",
    "version": "0.0.0-development",
    "description": "Node.js library for the Dintero API",
    "main": "./dist/dintero.js",
    "typings": "./dist/dintero.d.ts",
    "files": [
        "dist"
    ],
    "license": "MIT",
    "private": false,
    "publishConfig": {
        "access": "public"
    },
    "author": "Dintero <support@dintero.com> (https://dintero.com/)",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Dintero/Dintero.Node.SDK.git"
    },
    "homepage": "https://github.com/Dintero/Dintero.Node.SDK",
    "bugs": {
        "url": "https://github.com/Dintero/Dintero.Node.SDK/issues"
    },
    "jest": {
        "preset": "ts-jest",
        "testMatch": [
            "**/test/**/*.test.ts"
        ],
        "transform": {
            "node_modules/until-async/.*\\.js$": [
                "ts-jest",
                {
                    "tsconfig": {
                        "allowJs": true,
                        "esModuleInterop": true
                    }
                }
            ]
        },
        "transformIgnorePatterns": [
            "node_modules/(?!until-async)"
        ]
    },
    "scripts": {
        "build": "yarn -s build-ts && yarn -s lint",
        "build-ts": "tsc -p ./tsconfig.build.json",
        "test": "jest",
        "lint": "biome check",
        "convert-spec": "ts-node -T scripts/convert-spec.ts",
        "semantic-release": "semantic-release",
        "prepublishOnly": "yarn run build-ts"
    },
    "devDependencies": {
        "@biomejs/biome": "2.3.14",
        "@types/jest": "30.0.0",
        "@types/js-yaml": "4.0.9",
        "@types/node": "25.2.2",
        "@types/swagger2openapi": "7.0.4",
        "jest": "30.2.0",
        "js-yaml": "4.1.1",
        "msw": "2.12.9",
        "openapi-typescript": "7.10.1",
        "semantic-release": "25.0.3",
        "swagger2openapi": "7.0.8",
        "ts-jest": "29.4.6",
        "ts-node": "10.9.2",
        "typescript": "5.9.3"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "dependencies": {
        "openapi-fetch": "^0.15.0"
    }
}
